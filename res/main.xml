<?xml version="1.0" encoding="UTF-8"?>
<layout id="main" dockspace="true">
    <menubar>
    </menubar>
    <window name="Camera">
        <combo label="Select Camera" preview="$selected_camera">
            <custom event="select_camera" />
        </combo>
        <image src="$camera" />
    </window>
    <window name="Arm" />
    <window name="Sensors" />
    <window name="General Information">
        <button event="open_settings">Open Settings</button>
    </window>
    <window name="Controls">
        <combo label="Select Joystick" preview="$selected_joystick">
            <custom event="select_joystick" />
        </combo>
    </window>
</layout>